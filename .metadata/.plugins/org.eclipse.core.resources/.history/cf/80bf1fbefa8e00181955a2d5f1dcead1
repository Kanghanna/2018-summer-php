<?php

echo "점수에 따른 학점 판별<hr/>";
$score = 66;
$num = $score / 10;
echo "입력된 점수 : $score<br>";

switch($num){
case 0 : case 1 : case 2 : case 3 : case 4 : case 5 :
    $class = "F";
    break;
case 6 :
    $class = "D";
    break;
case 7 :
    $class = "C";
    break;
case 8 :
    $class = "B";
    break;
case 9 :
    $class = "A";
    break;
default;
}

    echo "학점 : $class";
    
    if($score >= 60 && $score % 10 >= 5 || $score == 100)
    echo "+";

?>